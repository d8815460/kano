    pod 'RealmSwift'
    pod 'LoadMoreTableViewController'
end

target 'KANOTests' do
    pod 'Alamofire'
    pod 'SDWebImage', '~> 4.0'
    pod 'RealmSwift'
    pod 'LoadMoreTableViewController'
end

target 'KANOUITests' do
    pod 'Alamofire'
    pod 'SDWebImage', '~> 4.0'
    pod 'RealmSwift'
    pod 'LoadMoreTableViewController'
end
